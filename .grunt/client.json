{
  "namespace": "Hull",
  "coffeeFiles": [
    "src/hullbase.coffee",
    "src/hull.coffee",
    "src/utils/**/*.coffee",
    "src/client/**/*.coffee",
    "src/api.coffee",
    "src/api/**/*.coffee"
  ],
  "requireJS": {
    "baseUrl": ".",
    "preserveLicenseComments": true,
    "paths": {
      "jquery":         "empty:",
      "underscore":     "bower_components/underscore/underscore",
      "aura":           "bower_components/aura/lib",
      "eventemitter":   "bower_components/eventemitter2/lib/eventemitter2",
      "backbone":       "bower_components/backbone/backbone",
      "xdm":            "bower_components/xdm.js/xdm",
      "handlebars":     "lib/shims/node_modules/grunt-contrib-handlebars/node_modules/handlebars/dist/handlebars",
      "requireLib":     "bower_components/requirejs/require",
      "cookie":         "bower_components/cookies-js/src/cookies",
      "promises":       "bower_components/rsvp/rsvp.amd",
      "string":         "bower_components/underscore.string/lib/underscore.string",
      "text":           "bower_components/requirejs-text/text",
      "base64":         "bower_components/base64/base64"
    },
    "shim": {
      "backbone":   { "exports": "Backbone", "deps": ["underscore", "jquery"] },
      "underscore": { "exports": "_" }
    },
    "include": [
      "requireLib",
      "underscore",
      "string",
      "cookie",
      "base64",
      "backbone",
      "text",
      "aura/ext/debug",
      "aura/ext/mediator",
      "aura/ext/components",
      "lib/hull"
      ],
    "out": "dist/<%= PKG_VERSION %>/hull.js",
    "wrap": {
      "start": "(function () {_chk = function (n,d) {if(!d){var _msg = n + \" is not defined. It is required to run hull.js\";alert(_msg); throw new Error(_msg);};return true;};_chk('jQuery', window.jQuery);var require, requirejs, define,root = {jQuery: window.jQuery};(function () {",
      "end": ";}).call(root);for(var i in root){_chk(i, root[i]) && root.hasOwnProperty(i) && Hull.define(i.toLowerCase(), function (){return root[i];})}})();"
    }
  }
}
