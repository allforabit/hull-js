{
  "coffeeFiles": ["src/hullbase.coffee", "src/hull.coffee", "src/hull-remote.coffee", "src/utils/**/*.coffee", "src/remote/**/*.coffee"],
  "requireJS": {
    "baseUrl": ".",
    "preserveLicenseComments": true,
    "paths": {
      "aura":               "bower_components/aura/lib",
      "underscore":         "bower_components/underscore/underscore",
      "eventemitter":       "bower_components/eventemitter2/lib/eventemitter2",
      "xdm":                "bower_components/xdm.js/xdm",
      "requireLib":         "bower_components/requirejs/require",
      "jquery":             "vendor/jquery-bare",
      "analytics":          "bower_components/analytics/analytics",
      "base64":             "bower_components/base64/base64"
    },
    "shim": {
      "underscore": { "exports": "_" },
      "analytics": { "exports": "analytics" }
    },
    "include": [
      "requireLib",
      "underscore",
      "xdm",
      "eventemitter",
      "base64",
      "analytics",
      "aura/ext/debug",
      "aura/ext/mediator",
      "lib/hull-remote"
    ],
    "out": "dist/<%= PKG_VERSION %>/hull-remote.js"
  }
}
