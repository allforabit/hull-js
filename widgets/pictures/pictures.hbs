<div class="hull-pictures">
  {{#if pictures}}
    <ul class="media-list">
      {{#each pictures}}
        <li class="media">
          <a class="pull-left" href="#">
            <img src="{{picture}}" alt="{{name}}" width="200" height="200">
          </a>
          <div class="media-body">
            <h4 class="media-heading">{{name}}</h4>
          </div>
        </li>
      {{/each}}
    </ul>
  {{else}}
    <div class="hull-empty">
      <p>Oopsâ€¦ No pictures for the moment</p>
    </div>
  {{/if}}
</div>


