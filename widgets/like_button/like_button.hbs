{{#if working}}
  <button class="btn hull-working">
    Loading... 
    <i class="icon-heart"></i>({{likes}})
  </button>
  
{{else}}
  {{#if liked}}
    <button data-hull-action="unlike" class="btn hull-liked">
      Unlike
      <i class="icon-heart"></i>({{likes}})
    </button>
  {{else}}
    <button data-hull-action="like" class="btn hull-like">
      Like
      <i class="icon-heart"></i>({{likes}})
    </button>
  {{/if}}
{{/if}}
